    "X_train = X_train.astype('float32')\n",
    "X_test = X_test.astype('float32')\n",
    "\n",
    "# Ensure that y_train and y_test are of type int (for binary classification)\n",
    "y_train = y_train.astype('int')\n",
    "y_test = y_test.astype('int')\n",
